{
    "name": "burnbright/omnipay-poli",
    "type": "library",
    "description": "POLi driver for the Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "polipay",
        "POLi",
        "payment"
    ],
    "homepage": "https://github.com/burnbright/omnipay-poli",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Shipman",
            "email": "jeremy@burnbright.net",
            "homepage": "http://jeremyshipman.com"
        },
        {
            "name": "Sid Bachtiar",
            "email": "sid.bachtiar@gmail.com",
            "homepage": "http://onlinesid.com"
        },
        {
            "name": "Toby Evans",
            "email": "tobz.nz@gmail.com",
            "homepage": "https://tobz.nz"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\Poli\\" : "src/"
        }
    },
    "require": {
        "php": ">=7.3",
        "omnipay/common": "^3.2",
        "symfony/http-client": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "mockery/mockery": "^1.5",
        "squizlabs/php_codesniffer": "^3.7",
        "omnipay/tests": "^4.1",
        "http-interop/http-factory-guzzle": "^1.2"
    },
    "extra": {
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
